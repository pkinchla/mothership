<article class="tease tease-{{post.post_type}}" id="tease-{{post.ID}}">
  {% block content %}
    <h4><a class="linked-header" href="{{post.link}}">{{post.title}}</a></h4>
    <div class="entry-meta">
      <time datetime="{{post.date|date('c')}}">{{post.date|date('l, F jS, Y')}}</time>
    </div>
  {% endblock %}
</article>