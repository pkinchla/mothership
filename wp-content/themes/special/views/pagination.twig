<div class="pagination js-pagination-block">
  {% if pagination.prev %}
  <a href="{{pagination.prev.link}}" class="prev {{pagination.prev.link|length ? '' : 'invisible'}} js-pagination">Prev</a>
  {% endif %}
  {% for page in pagination.pages %}
    {% if page.link %}
      <a href="{{page.link}}" class="{{page.class}} js-pagination">{{page.title}}</a>
    {% else %}
      <span class="{{page.class}}">{{page.title}}</span>
    {% endif %}
    </li>
  {% endfor %}
  </ul>
  {% if pagination.next %}
    <a href="{{pagination.next.link}}" class="next {{pagination.next.link|length ? '' : 'invisible'}} js-pagination">Next</a>
  {% endif %}
</div>
